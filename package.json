    "electron-package-linux": "ng build --prod --base-href=./ && electron-packager . ArrayExplorer --overwrite  --platform=linux --arch=x64  --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds --asar"
